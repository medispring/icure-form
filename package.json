	"main": "lib/index.js",
	"scripts": {
		"start": "node lit-inject-scss.js src tmp && webpack-dev-server",
		"build": "node lit-inject-scss.js src tmp && jq '{include:[\"tmp\"], compilerOptions:.compilerOptions}' < tsconfig.json > tsconfig.tmp.json && tsc -p tsconfig.tmp.json && rm -r tmp",
		"dist": "webpack --env.mode production",
		"prepare": "rm -r lib && yarn run build && jq '{name:.name, version:.version, description:.description, main:.main, types:.types, dependencies:.dependencies, author:.author, license:.license, bugs:.bugs, homepage:.homepage}' < package.json > lib/package.json",
		"publish": "yarn version patch && yarn run prepare && cd dist && yarn npm publish --tolerate-republish --access public && cd ..",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "",
	"license": "GPL-3.0",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix",
			"git add"
		]
	},
