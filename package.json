{
	"name": "@icure/form",
	"version": "2.0.5",
	"description": "",
	"main": "lib/index.js",
	"scripts": {
		"start": "webpack serve --mode development",
		"build": "node lit-inject-scss.js src tmp && jq '{include:[\"tmp\"], compilerOptions:.compilerOptions}' < tsconfig.json > tsconfig.tmp.json && tsc -p tsconfig.tmp.json",
		"dist": "webpack --env.mode production",
		"prepare": "rm -r lib && yarn run build && jq '{name:.name, version:.version, description:.description, main:.main, types:.types, dependencies:.dependencies, author:.author, license:.license, bugs:.bugs, homepage:.homepage}' < package.json > lib/package.json",
		"publish": "yarn version patch && yarn run prepare && cd lib && touch yarn.lock && yarn install && rm -rf node_modules && npm publish --tolerate-republish --access public && cd ..",
		"test": "jest",
		"lint:commit": "eslint . --ext .ts",
		"pre-commit": "lint-staged"
	},
	"keywords": [
		"litElement",
		"ProseMirror",
		"node",
		"typescript"
	],
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node",
			"yaml"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run pre-commit"
		}
	},
	"lint-staged": {
		"*.ts": [
			"eslint --fix",
			"git add"
		]
	},
	"author": "",
	"license": "GPL-3.0",
	"access": "public",
	"dependencies": {
		"@webcomponents/webcomponentsjs": "^2.7.0",
		"app-datepicker": "^6.0.0-rc.33",
		"fast-deep-equal": "^3.1.3",
		"lit": "^3.1.3",
		"markdown-it": "^14.1.0",
		"orderedmap": "2.1.1",
		"prosemirror-commands": "1.5.2",
		"prosemirror-history": "1.3.2",
		"prosemirror-keymap": "1.2.2",
		"prosemirror-markdown": "1.12.0",
		"prosemirror-model": "1.19.4",
		"prosemirror-schema-basic": "1.2.2",
		"prosemirror-schema-list": "1.3.0",
		"prosemirror-state": "1.4.3",
		"prosemirror-transform": "1.8.0",
		"prosemirror-view": "1.32.7",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@icure/api": "5.3.7",
		"@types/jest": "^27.0.2",
		"@types/markdown-it": "^12.2.3",
		"@types/node": "^18.16.3",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"assert": "^2.0.0",
		"babel": "^6.23.0",
		"babel-eslint": "10.1.0",
		"babel-preset-es2017": "^6.24.1",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.3",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsdoc": "^40.0.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"extract-loader": "^5.1.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^8.0.3",
		"jest": "^27.1.0",
		"lint-staged": "^13.1.2",
		"lit-scss-loader": "^2.0.1",
		"minisearch": "^6.0.1",
		"prettier": "^2.8.4",
		"pretty-quick": "^3.1.3",
		"raw-loader": "^4.0.2",
		"sass": "^1.58.3",
		"sass-loader": "^13.2.0",
		"ts-jest": "^27.0.4",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.4.5",
		"webpack": "5.88.2",
		"webpack-cli": "5.1.4",
		"webpack-dev-server": "4.15.1",
		"yaml": "2.3.4"
	},
	"resolutions": {
		"prosemirror-model": "1.19.4",
		"prosemirror-state": "1.4.3",
		"prosemirror-view": "1.32.7",
		"@lit/reactive-element": "1.6.2",
		"lit-element": "3.3.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"types": "./lib/index.d.ts",
	"directories": {
		"lib": "lib",
		"test": "test"
	}
}
